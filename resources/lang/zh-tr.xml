				</pageHeader>
				<pageEntry vars="COMMAND_NAME,COMMAND_DESCRIPTION">//${COMMAND_NAME}︰${COMMAND_DESCRIPTION}</pageEntry>
			</help>
		</command>
		<phrases>
		</phrases>
	</values>
